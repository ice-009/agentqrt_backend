        <div class="container ml-lg-2 mt-2">
          <div class="title">Create Organization</div>
          <div class="content">
            <form action="./create" method="post">
              <div class="user-details">
                <div class="input-box">
                  <span class="details">Organization Name</span>
                  <input name="orgname" type="text" placeholder="Enter your name" required="true">
                </div>
                <div class="input-box">
                  <span class="details">Username</span>
                  <input name="username" type="text" placeholder="Enter your username" required="true">
                </div>
                <div class="input-box">
                  <span class="details">Email</span>
                  <input name="email" type="text" placeholder="Enter your email" required="true">
                </div>
                <div class="input-box">
                  <span class="details">Gst no</span>
                  <input name="gstno" type="text" placeholder="Enter gst no" required="true">
                </div>
                <div class="input-box">
                  <span class="details">Address</span>
                  <input name="address" type="text" placeholder="Enter address" required="true">
                </div>
                <div class="input-box">
                  <span class="details">State</span>
                  <input name="state" type="text" placeholder="Enter state" required="true">
                </div>
                <div class="input-box">
                  <span class="details">Pin code</span>
                  <input name="pincode" type="number" placeholder="Enter pincode" required="true">
                </div>
                <div class="input-box">
                  <span class="details">Year of Registration</span>
                  <input name="yor" type="number" placeholder="Enter year of registration">
                </div>

                <div class="input-box">
                  <span class="details">Contact Person name</span>
                  <input name="contactperson" type="text" placeholder="Enter contact person" required="true">
                </div>

                <div class="input-box">
                  <span class="details">Contact no</span>
                  <input name="contactnumber" type="text" placeholder="Enter contact no" required="true">
                </div>

                <div class="input-box">
                  <span class="details">Website</span>
                  <input name="website" type="text" placeholder="Enter website" required="true">
                </div>

                <div class="input-box">
                  <span class="details">Password</span>
                  <input name="password" type="text" placeholder="Enter your password" required="true">
                </div>
                <div class="input-box">
                  <span class="details">Confirm Password</span>
                  <input name="cpassword" type="text" placeholder="Confirm your password" required="true">
                </div>
              </div>

              <div class="button">
                <input type="submit" value="Register">
              </div>
            </form>
